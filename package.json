{
    "name": "@wxn0brp/db-storage-dir",
    "version": "0.2.0-alpha.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "wxn0brP",
    "license": "MIT",
    "type": "module",
    "description": "ValtheraDB Dir Storage",
    "repository": {
        "type": "git",
        "url": "https://github.com/wxn0brP/ValtheraDB-storage-dir.git"
    },
    "homepage": "https://github.com/wxn0brP/ValtheraDB",
    "keywords": [
        "valthera",
        "valtheradb",
        "vdb-adapter"
    ],
    "scripts": {
        "build": "tsc && tsc-alias"
    },
    "dependencies": {
        "json5": "^2.2.3"
    },
    "peerDependencies": {
        "@wxn0brp/db-core": "~0.5.0"
    },
    "devDependencies": {
        "@types/bun": "*",
        "@wxn0brp/db-core": "~0.5.0",
        "tsc-alias": "*",
        "typescript": "*"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "default": "./dist/*.js"
        }
    }
